  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "npm run lint:fix",
      "npm run lint:format"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "browserslist": [
    "chrome >= 80",
    "firefox >= 74",
    "safari >= 10",
    "edge >= 80",
    "ios >= 10",
    "ie >= 8"
  ]
}
